Host *
    IdentityAgent "~/.1password/agent.sock"
    TCPKeepAlive no
    ServerAliveInterval 60
    ServerAliveCountMax 10
    GSSAPIAuthentication no
    StrictHostKeyChecking no
    ControlMaster no
    ControlPath none
    ForwardAgent yes

Include ~/.ssh/ssh_hosts_personal
Include ~/.ssh/ssh_hosts_work
